int main()
{
    int x;
    x =10;
    read(x);
    
    write(x, "! = ", recursiveFactorial(x));
    newline;
    newline;
    counter(30);

    isPrime(91);
    isPrime(6);
    isPrime(7);
    return 0;
}

int recursiveFactorial(int n) {
    
    if (n>=1){
        return n*recursiveFactorial(n-1);
    }
    else{
        return 1;
    }
}

int counter(int x){ 
    int i;
    i = 0;
    while(i<x){
        write(i,"\n");
        i = i + 1;

    }
    write("END OF LOOP\n");
    return i;
}

int isPrime(int x){

    int i;
    i = 2;

    while(i < x/2){
        if(x % 1 == 0){
            write(x, " is a prime number\n");
            return 1;
        }
    }

    write(x, " isn't a prime number\n");
    return -1;
}

